{
  "name": "corgi",
  "version": "1.0.0",
  "description": "Tools for recording time with one click.",
  "main": "main.js",
  "scripts": {
    "start": "electron ."
  },
  "build": {
    "appId": "com.electron.corgi",
    "productName": "Corgi",
    "files": [
      "build/*",
      "resources/*",
      "./main.js",
      "./index.html"
    ],
    "mac": {
      "target": "dmg",
      "icon": "build/icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.png"
    }
  },
  "author": "strelka145",
  "license": "MIT",
  "devDependencies": {
    "electron": "^23.1.1",
    "electron-builder": "^23.6.0"
  }
}
